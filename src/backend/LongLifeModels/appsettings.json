{
  "OpenAI": {
    "ApiKey": "",
    "BaseUrl": "https://api.openai.com",
    "ChatModel": "gpt-4o-mini",
    "EmbeddingModel": "text-embedding-3-small"
  },
  "Qdrant": {
    "BaseUrl": "http://localhost:6333",
    "CollectionName": "memory_logs",
    "ApiKey": "",
    "VectorSize": 1536,
    "Distance": "Cosine"
  },
  "MemoryCompression": {
    "MaxMemoryLogsPerAgent": 500,
    "CompressionBatchSize": 25,
    "ContextTokenLimit": 8000,
    "SummaryImportance": 0.7
  },
  "Simulation": {
    "TickIntervalMs": 1000,
    "SpeedFactor": 1.0,
    "MaxAgentsPerTick": 100,
    "MessageTypingDelayMs": 50 
  },
  "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
  },
  "AllowedHosts": "*"
}
